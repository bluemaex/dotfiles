<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
	<dict>
		<key>Label</key>
		<string>de.blmx.bingwallpaper</string>
		<key>ProgramArguments</key>
		<array>
			<string>/bin/bash</string>
			<string>/usr/local/bin/bing-wallpaper</string>
			<string>--set-wallpaper</string>
		</array>
		<key>LowPriorityIO</key>
		<true/>
		<key>Nice</key>
		<integer>1</integer>
		<key>StartCalendarInterval</key>
			<array>
				<dict>
					<key>Hour</key>
					<integer>10</integer>
					<key>Minute</key>
					<integer>0</integer>
				</dict>
				<dict>
					<key>Hour</key>
					<integer>16</integer>
					<key>Minute</key>
					<integer>0</integer>
				</dict>
			</array>
	</dict>
</plist>
